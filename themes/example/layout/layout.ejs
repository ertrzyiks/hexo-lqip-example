<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
    <%
    var title = page.title;

    if (is_category()){
      title = __('category') + ': ' + page.category;
    }
    %>
  <title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <%- css('css/style') %>
</head>
<body>
  <section id="main">
      <%- body %>
  </section>

  <%- js('js/script') %>
</body>
</html>
